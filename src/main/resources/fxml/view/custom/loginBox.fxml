<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml">
    <HBox>
        <children>
            <Label prefHeight="20.0" prefWidth="75.0" text="User:" />
            <TextField fx:id="user" prefHeight="20.0" prefWidth="200.0" />
        </children>
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </VBox.margin>
    </HBox>
    <HBox>
        <children>
            <Label prefHeight="20.0" prefWidth="75.0" text="Password:" />
            <PasswordField fx:id="password" prefHeight="20.0" prefWidth="200.0" />
        </children>
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </VBox.margin>
    </HBox>
</fx:root>
